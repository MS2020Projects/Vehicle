                        <div class="form-group">
                            <label for="comments">Comments</label>
                            <textarea class="form-control input-sm" name="comments" id="comments" value="@Model.Comments"></textarea>
                        </div>
                    </div>
                </div>

                <div class="row">

                    <div class="col-sm-4">
                        <div class="form-group">
                            <label for="electrical">Electrical</label>
                            <input class="form-control input-sm" id="electrical" type="text" value="@Model.Electrical">
                        </div>
                    </div>
                    <div class="col-sm-4">
                        <div class="form-group">
                            <label for="service">Service</label>
                            <textarea class="form-control input-sm" id="service" name="service" value="@Model.Service"></textarea>
                        </div>
                    </div>
                    <div class="col-sm-4">
                        <div class="form-group">
                            <label for="bodyrepair">Body Repair</label>
                            <textarea class="form-control input-sm" id="bodyrepair" name="bodyrepair" value="@Model.BodyRepair"></textarea>
                        </div>
                    </div>
                </div>

            </div>

            <div class="panel-body no-padding" style="display: block;">
                <h2>Heading 2</h2>
                <div class="row">
                    <div class="col-sm-4">
                        <div class="form-group">
