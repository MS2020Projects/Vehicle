                            <input class="form-control input-sm" name="mechanical" type="text" value="@Model.Mechanical">
                        </div>
                    </div>
                </div>

                <div class="row">

                    <div class="col-sm-3">
                        <div class="form-group">
                            <label for="additional">Additional</label>
                            <input class="form-control input-sm" name="additional" type="text" value="@Model.Additional">
