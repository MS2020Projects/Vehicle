                    </div>
                </div>
                <div class="col-sm-3">
                    <div class="form-group">
                        <label for="jobdate">Date</label>
                        <input class="form-control input-sm" name="jobdate" type="date" value="@Model.JobDate">
                    </div>
                </div>
                <div class="col-sm-3">
                    <div class="form-group">
                        <label for="vehicleregnum">Vehicle Number</label>
                        <input class="form-control input-sm" name="vehicleregnum" type="number" value="@Model.VehicleNumber">
                    </div>
                </div>
                <div class="col-sm-3">
