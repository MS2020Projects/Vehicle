                            <input class="form-control input-sm" id="estimatenum" name="estimatenum" type="number" value="@Model.EstimateNumber">

                        </div>
                    </div>
                    <div class="col-sm-3">
                        <div class="form-group">
                            <label for="amount">Amount</label>

                            <input class="form-control input-sm" id="amount" name="amount" type="number" value="@Model.Amount">

