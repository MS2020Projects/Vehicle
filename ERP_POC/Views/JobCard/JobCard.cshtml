                            <label for="modify">Modify</label>&nbsp;&nbsp;<input name="modify" type="radio" value="@Model.Modify">
                        </div>
                    </div>
                    <div class="col-sm-4"></div>
                </div>
                <div class="row">

                    <div class="col-sm-3">
                        <div class="form-group">
                            <label for="make">Make</label>
                            <input class="form-control input-sm" name="make" type="text" value="@Model.Make">
                        </div>
                    </div>
                    <div class="col-sm-3">
                        <div class="form-group">
                            <label for="modelnum">Model</label>
                            <input class="form-control input-sm" name="modelnum" type="number" value="@Model.Model">
                        </div>
                    </div>
                    <div class="col-sm-3">
