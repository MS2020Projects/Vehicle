                            <label for="invoicenum">Invoice No.</label>
                            <input class="form-control input-sm" name="invoicenum" type="number" value="@Model.InvoiceNumber">
                        </div>
                    </div>
                    <div class="col-sm-3">
                        <div class="form-group">
                            <label for="invoicedate">Invoice Date</label>
                            <input class="form-control input-sm" name="invoicedate" type="date" value="@Model.InvoiceDate">
