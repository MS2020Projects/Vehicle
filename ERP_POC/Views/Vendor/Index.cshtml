@model ERP_POC.Models.VendorVM
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="graphs">
    <div class="xs tabls">
        <div class="div_Create">
            @Html.Partial("Create", Model.Vendor)
        </div>
         

        <div class="div_List">
            @Html.Partial("List", Model.Vendors)
        </div>
    </div>
</div>

<script>
    function LoadList() {
        alert("1");
        var JsonData = {
            URL: "../Vendor/List",
            TYPE: "Get",
            DATATYPE: 'html',
            PartialDiv: ".div_List",

        }

        LoadPartialDiv(JsonData);
    }
</script>


